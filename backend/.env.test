# テスト環境設定
NODE_ENV=test

# データベース設定
DATABASE_URL=postgresql://postgres:password@localhost:5432/ai_agent_dev

# Redis設定
REDIS_URL=redis://localhost:6379

# JWT設定
JWT_SECRET=test-jwt-secret-key
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# API Keys
OPENAI_API_KEY=test-openai-api-key
ANTHROPIC_API_KEY=test-anthropic-api-key

# Zoom API設定
ZOOM_API_KEY=test-zoom-api-key
ZOOM_API_SECRET=test-zoom-api-secret
ZOOM_WEBHOOK_SECRET=test-zoom-webhook-secret

# メール設定
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=test@example.com
SMTP_PASS=test-password
SMTP_FROM=Test Service <test@example.com>

# アプリケーション設定
PORT=8001
FRONTEND_URL=http://localhost:3000

# ログ設定
LOG_LEVEL=silent